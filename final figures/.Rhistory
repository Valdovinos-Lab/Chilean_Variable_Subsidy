y3 <- smooth.spline(x,fig$'c3', spar = 0.4)$y
figdf <- data.frame(x,y1,y2,y3)
dates <- c(1, 30+31+1, 30+31+30+31+1)
dates <- dates*24-23
months <- c("Sept1","Nov1","Jan1")
#group.colors <- c('op' = "#62D8DB", 'fp' = "#1B9E77")
graph <- ggplot(figdf, aes(x=x)) +
geom_line(aes(y = y1), color = "#59c4c7", size = 1.2) +
geom_line(aes(y = y2), color = "#188c6a", size = 1.2) +
geom_line(aes(y = y3), color = "#605c91", size = 1.2) +
theme(legend.position = "none", text = element_text(size=30)) +
scale_x_continuous(breaks = dates, labels = months) +
scale_y_continuous(labels = scales::scientific, limits = c(0,150000)) +
labs(x = "", y = "")
print(graph)
